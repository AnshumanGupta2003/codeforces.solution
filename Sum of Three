for _ in range(int(input())):
    n=int(input())
    if n == 1 or n == 2 or n == 3 or n == 4 or n == 5 or n == 6 or n == 9:
        print("NO")
    elif n == 10:
        print("YES\n1 2 7")
    elif n%3==0:
        print("YES")
        print("1 4",n-5)
    elif n%3==1:
        print("YES")
        print("2 4",n-6)
    elif n%3==2:
        print("YES")
        print("1 2",n-3)


